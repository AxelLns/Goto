{
    "tailwindCSS.experimental.classRegex": [
        "tw`([^`]*)", // tw`...`
        "tw=\"([^\"]*)", // <div tw="..." />
        "tw={\"([^\"}]*)", // <div tw={"..."} />
        "tw\\.\\w+`([^`]*)", // tw.xxx`...`
        "tw\\(.*?\\)`([^`]*)", // tw(Component)`...`
    ],
    "typescript.suggest.paths": false,
    "javascript.suggest.paths": false,
    "typescript.tsdk": "node_modules/typescript/lib",
    "javascript.validate.enable": false
}